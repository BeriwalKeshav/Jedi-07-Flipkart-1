/**
 * 
 */
package com.flipkart.exception;

/**
 * @author ankit
 *
 */
public class CourseNotOfferedException {
	private String cCode;

	public CourseNotOfferedException(String cCode) {
		this.cCode = cCode;
	}
	
	public String getMessage() {
		return "Course with code " + cCode + "is not present in the Catalog";
	}

}
